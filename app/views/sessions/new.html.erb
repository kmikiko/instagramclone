<h1>Instagram</h1>
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
  <table>
    <tr>
      <td>
        <%= f.label :email %>
        <%= f.email_field :email %>
      </td>
    </tr>
    <tr>
      <td>
        <%= f.label :password %>
        <%= f.password_field :password %>
      </td>
    </tr>
    <tr>
      <td>
        <%= f.submit "ログイン" %>
      </td>
    </tr>
  </table>
<% end %>
